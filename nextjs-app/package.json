{
  "name": "7iatlas",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "test": "jest --testPathIgnorePatterns=e2e",
    "test:watch": "jest --watch --testPathIgnorePatterns=e2e",
    "test:coverage": "jest --coverage --testPathIgnorePatterns=e2e",
    "test:unit": "jest __tests__/unit",
    "test:integration": "jest __tests__/integration",
    "test:e2e": "dotenv -e .env.test -- jest __tests__/e2e --runInBand",
    "test:e2e:setup": "dotenv -e .env.test -- npx ts-node --transpile-only scripts/setup-test-db.ts",
    "test:all": "npm run test && npm run test:e2e",
    "db:generate": "prisma generate",
    "db:push": "prisma db push",
    "db:push:test": "dotenv -e .env.test -- prisma db push",
    "db:migrate": "prisma migrate dev",
    "db:seed": "ts-node prisma/seed.ts"
  },
  "dependencies": {
    "@prisma/client": "^5.7.0",
    "@rainbow-me/rainbowkit": "^1.3.0",
    "bcryptjs": "^2.4.3",
    "ethers": "^6.9.0",
    "jsonwebtoken": "^9.0.2",
    "next": "14.0.4",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "viem": "^1.19.0",
    "wagmi": "^1.4.12",
    "zustand": "^4.4.7"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.0",
    "@types/bcryptjs": "^2.4.6",
    "@types/jest": "^30.0.0",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/node": "^20.10.0",
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "autoprefixer": "^10.4.16",
    "dotenv-cli": "^11.0.0",
    "eslint": "^8.55.0",
    "eslint-config-next": "14.0.4",
    "jest": "^30.2.0",
    "jest-mock-extended": "^4.0.0",
    "postcss": "^8.4.32",
    "prisma": "^5.7.0",
    "tailwindcss": "^3.4.0",
    "ts-jest": "^29.4.6",
    "typescript": "^5.3.0"
  }
}
